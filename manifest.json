{
	"name": "sk8tracker",
	"version": "1.0",
	"description": "Track and improve your skateboard progression",
	"permissions": ["storage"],
	"page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
	"chrome_url_overrides" : {
    "newtab": "main.html"
  },
	"manifest_version": 2
}